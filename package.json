{
  "name": "@rxts/koa-router-decorators",
  "version": "0.1.2",
  "description": "Decorators for koa-router, inspired by Java Annotations of Spring MVC framework",
  "repository": "git@github.com:rx-ts/koa-router-decorators.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "lib/cjs",
  "module": "lib",
  "es2015": "lib/es2015",
  "fesm5": "lib/esm",
  "types": "lib",
  "files": [
    "lib",
    "!*.tsbuildinfo"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:r": "r -p",
    "build:ts": "tsc",
    "dev": "tsnd --respawn -O '{\"module\":\"commonjs\"}' -T server",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext md,ts",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "yarn-deduplicate || exit 0",
    "prepublishOnly": "yarn build",
    "type-coverage": "type-coverage --cache --detail --ignore-catch --strict"
  },
  "peerDependencies": {
    "koa": ">=2.0.0",
    "koa-router": ">=7.0.0"
  },
  "devDependencies": {
    "@1stg/lib-config": "^0.1.15",
    "@types/koa": "^2.11.0",
    "@types/koa-router": "^7.0.42",
    "@types/koa-session": "^5.10.1",
    "@types/node": "^13.1.4",
    "consola": "^2.11.3",
    "koa": "^2.11.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.3",
    "npm-run-all": "^4.1.5",
    "ts-node-dev": "^1.0.0-pre.44",
    "type-coverage": "^2.4.0",
    "typescript": "^3.7.4",
    "yarn-deduplicate": "^1.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "typeCoverage": {
    "atLeast": 93
  }
}
