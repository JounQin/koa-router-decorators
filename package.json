{
  "name": "koa-router-decorators",
  "version": "0.0.1",
  "description":
    "Decorators for koa-router, inspired by Java Annotations of Spring MVC framework",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": "git@github.com:JounQin/koa-router-decorators.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "build:cjs": "tsc -P lib -m commonjs --outDir dist/cjs",
    "build:esm": "tsc -P lib -m esnext --outDir dist/esm",
    "build": "concurrently 'npm run build:cjs' 'npm run build:esm'",
    "dev": "nodemon --exec 'cross-env DEBUG=1stg:* ts-node -P . src/index.ts'",
    "lint": "tslint -p . -e node_moudles/* -t stylish",
    "precommit": "lint-staged"
  },
  "files": ["dist"],
  "lint-staged": {
    "*.{md,json,ts}": ["prettier --write", "git add"],
    "*.ts": ["tslint -p . -t stylish"]
  },
  "nodemonConfig": {
    "watch": ["lib/*.*", "src/*.*"]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "peerDependencies": {
    "@types/koa": "^2.0.40",
    "@types/koa-router": "^7.0.25",
    "koa": "^2.4.1",
    "koa-router": "^7.3.0",
    "typescript": "^2.6.1"
  },
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/koa": "^2.0.41",
    "@types/koa-router": "^7.0.26",
    "@types/koa-session": "^5.7.1",
    "@types/node": "^9.3.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.1",
    "debug": "^3.1.0",
    "husky": "^0.14.3",
    "koa": "^2.4.1",
    "koa-router": "^7.3.0",
    "koa-session": "^5.5.1",
    "lint-staged": "^7.0.0",
    "nodemon": "^1.12.1",
    "prettier": "^1.8.2",
    "ts-node": "^4.0.1",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.2"
  }
}
