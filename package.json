{
  "name": "@rxts/koa",
  "version": "0.0.0",
  "description": "Incredible Koa middlewares/plugins, make Koa greater",
  "repository": "git@github.com:rx-ts/koa-router-decorators.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "lib/cjs",
  "module": "lib",
  "es2015": "lib/es2015",
  "fesm5": "lib/esm",
  "types": "lib",
  "files": [
    "lib",
    "index.ts",
    "!*.tsbuildinfo"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:r": "r -p",
    "build:ts": "tsc -b",
    "dev": "nodemon",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext md,ts",
    "lint:ts": "tslint -p . -t stylish",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "yarn-deduplicate || exit 0",
    "type-coverage": "type-coverage --cache --detail --ignore-catch --ignore-files '**/*.d.ts' --strict"
  },
  "peerDependencies": {
    "koa": ">=2.0.0",
    "koa-router": ">=7.0.0"
  },
  "devDependencies": {
    "@1stg/lib-config": "^0.1.19",
    "@1stg/tslint-config": "^0.5.7",
    "@types/http-proxy": "^1.17.2",
    "@types/koa": "^2.11.0",
    "@types/koa-compose": "^3.2.5",
    "@types/koa-router": "^7.0.42",
    "@types/koa-session": "^5.10.1",
    "@types/node": "^13.1.5",
    "consola": "^2.11.3",
    "koa": "^2.11.0",
    "koa-compose": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.3",
    "lerna": "^3.20.2",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "type-coverage": "^2.4.0",
    "typescript": "^3.7.4",
    "yarn-deduplicate": "^1.1.1"
  },
  "nodemonConfig": {
    "ext": ".ts",
    "exec": "ts-node -r tsconfig-paths/register -O '{\"module\":\"commonjs\"}' -T server",
    "ignore": "packages/lib",
    "watch": [
      "packages",
      "server"
    ]
  },
  "typeCoverage": {
    "atLeast": 88
  }
}
